@{
    // var currentUser =  (AppUser)ViewData["currentUser"];
}
<!DOCTYPE html>
<html lang="en">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Natural & Nutritious - @ViewData["Title"]</title>
    <!-- plugins:css -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css" rel="stylesheet">
    <link rel="stylesheet" href="~/admin/vendors/mdi/css/materialdesignicons.min.css">
    <link rel="stylesheet" href="~/admin/vendors/base/vendor.bundle.base.css">
    @RenderSection("SeeMessageCss", false)
    @RenderSection("MyOrderDetailsCss", false)
    <!-- endinject -->
    <!-- plugin css for this page -->
    <link rel="stylesheet" href="~/admin/vendors/datatables.net-bs4/dataTables.bootstrap4.css">
    <!-- End plugin css for this page -->
    <!-- inject:css -->
    <link rel="stylesheet" href="~/admin/css/style.css">
    <!-- endinject -->
    @*     <link rel="shortcut icon" href="images/favicon.png" /> *@
</head>
<body>
    <div class="container-scroller">
        <!-- partial:partials/_navbar.html -->
        <nav class="navbar col-lg-12 col-12 p-0 fixed-top d-flex flex-row">
            <div class="navbar-brand-wrapper d-flex justify-content-center">
                <div class="navbar-brand-inner-wrapper d-flex justify-content-between align-items-center w-100">
                    <a asp-action="Index" asp-controller="Home" class="navbar-brand brand-logo">
                        <div class="col-lg-4">
                            <h1 class="m-0 display-5 text-uppercase"><span class="text-primary">Natural & </span>Nutritious</h1>
                        </div>
                    </a>

                    <a class="navbar-brand brand-logo-mini" href="index.html"><img src="images/logo-mini.svg" alt="logo" /></a>
                    <button class="navbar-toggler navbar-toggler align-self-center" type="button" data-toggle="minimize">
                        <span class="mdi mdi-sort-variant"></span>
                    </button>
                </div>
            </div>
            <div class="navbar-menu-wrapper d-flex align-items-center justify-content-end">
                <ul class="navbar-nav navbar-nav-right">
                    <li class="nav-item nav-profile dropdown">
                        <a class="nav-link dropdown-toggle" href="#" data-bs-toggle="dropdown" id="profileDropdown">
                            <span class="nav-profile-name">Admin </span>
                        </a>
                        <div class="dropdown-menu dropdown-menu-right navbar-dropdown" aria-labelledby="profileDropdown">
                            <a asp-action="LogOut" asp-controller="Auth" class="dropdown-item">
                                <i class="mdi mdi-logout text-primary"></i>
                                Logout
                            </a>
                        </div>
                    </li>
                </ul>
                <button class="navbar-toggler navbar-toggler-right d-lg-none align-self-center" type="button" data-toggle="offcanvas">
                    <span class="mdi mdi-menu"></span>
                </button>
            </div>
        </nav>
        <!-- partial -->
        <div class="container-fluid page-body-wrapper">
            <!-- partial:partials/_sidebar.html -->
            <nav class="sidebar sidebar-offcanvas" id="sidebar">
                <ul class="nav">
                    <li class="nav-item">
                        <a class="nav-link active" asp-action="Index" asp-controller="Home">
                            <i class="mdi mdi-home menu-icon"></i>
                            <span class="menu-title">Dashboard</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" data-bs-toggle="collapse" href="#ui-users" aria-expanded="false" aria-controls="ui-users">
                            <i class="mdi mdi-account menu-icon"></i>
                            <span class="menu-title">Users</span>
                            <i class="menu-arrow"></i>
                        </a>
                        <div class="collapse" id="ui-users">
                            <ul class="nav flex-column sub-menu">
                                <li class="nav-item"> <a class="nav-link" asp-action="GetAllUsers" asp-controller="Users">All Users</a></li>
                                <li class="nav-item"> <a class="nav-link" asp-action="Create" asp-controller="Users">Create Users</a></li>
                                <li class="nav-item"> <a class="nav-link" asp-action="SendBatchEmail" asp-controller="Users">Send Batch Email</a></li>
                            </ul>
                        </div>
                    </li> 
                    <li class="nav-item">
                        <a class="nav-link" data-bs-toggle="collapse" href="#ui-subscribers" aria-expanded="false" aria-controls="ui-subscribers">
                            <i class="mdi mdi-account menu-icon"></i>
                            <span class="menu-title">Subscribers</span>
                            <i class="menu-arrow"></i>
                        </a>
                        <div class="collapse" id="ui-subscribers">
                            <ul class="nav flex-column sub-menu">
                                <li class="nav-item"> <a class="nav-link" asp-action="GetAllSubscribers" asp-controller="Subscribers">All Subscribers</a></li>
                                <li class="nav-item"> <a class="nav-link" asp-action="Create" asp-controller="Subscribers">Create Subscribers</a></li>
                                <li class="nav-item"> <a class="nav-link" asp-action="SendBatchEmail" asp-controller="Subscribers">Send Batch Email</a></li>
                            </ul>
                        </div>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" data-bs-toggle="collapse" href="#ui-messages" aria-expanded="false" aria-controls="ui-messages">
                            <i class="mdi mdi-message-processing menu-icon"></i>
                            <span class="menu-title">Contact Messages</span>
                            <i class="menu-arrow"></i>
                        </a>
                        <div class="collapse" id="ui-messages">
                            <ul class="nav flex-column sub-menu">
                                <li class="nav-item"> <a class="nav-link" asp-action="GetAllMessages" asp-controller="ContactMessages">All Messages</a></li>
                            </ul>
                        </div>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" data-bs-toggle="collapse" href="#ui-blogs" aria-expanded="false" aria-controls="ui-blogs">
                            <i class="mdi mdi-newspaper menu-icon"></i>
                            <span class="menu-title">Blogs</span>
                            <i class="menu-arrow"></i>
                        </a>
                        <div class="collapse" id="ui-blogs">
                            <ul class="nav flex-column sub-menu">
                                <li class="nav-item"> <a class="nav-link" asp-action="GetAllBlogs" asp-controller="Blogs">All Blogs</a></li>
                                <li class="nav-item"> <a class="nav-link" asp-action="Create" asp-controller="Blogs">Create Blogs</a></li>
                            </ul>
                        </div>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" data-bs-toggle="collapse" href="#ui-products" aria-expanded="false" aria-controls="ui-products">
                            <i class="mdi mdi-package menu-icon"></i>
                            <span class="menu-title">Products</span>
                            <i class="menu-arrow"></i>
                        </a>
                        <div class="collapse" id="ui-products">
                            <ul class="nav flex-column sub-menu">
                                <li class="nav-item"> <a class="nav-link" asp-action="GetAllProducts" asp-controller="Products">All Products</a></li>
                                <li class="nav-item"> <a class="nav-link" asp-action="Create" asp-controller="Products">Create Products</a></li>
                            </ul>
                        </div>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" data-bs-toggle="collapse" href="#ui-orders" aria-expanded="false" aria-controls="ui-orders">
                            <i class="mdi mdi-check-decagram menu-icon"></i>
                            @*  *@
                            <span class="menu-title">Orders</span>
                            <i class="menu-arrow"></i>
                        </a>
                        <div class="collapse" id="ui-orders">
                            <ul class="nav flex-column sub-menu">
                                <li class="nav-item"> <a class="nav-link" asp-action="GetAllOrders" asp-controller="Orders">All Orders</a></li>
                                <li class="nav-item"> <a class="nav-link" asp-action="GetUnconfirmedOrders" asp-controller="Orders">Unconfirmed Orders</a></li>
                                <li class="nav-item"> <a class="nav-link" asp-action="GetConfirmedOrders" asp-controller="Orders">Confirmed Orders</a></li>
                            </ul>
                        </div>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" data-bs-toggle="collapse" href="#ui-categories" aria-expanded="false" aria-controls="ui-categories">
                            <i class="mdi mdi-table menu-icon"></i>
                            @* mdi-table-large *@
                            <span class="menu-title">Categories</span>
                            <i class="menu-arrow"></i>
                        </a>
                        <div class="collapse" id="ui-categories">
                            <ul class="nav flex-column sub-menu">
                                <li class="nav-item"> <a class="nav-link" asp-action="GetAllCategories" asp-controller="Categories">All Categories</a></li>
                                <li class="nav-item"> <a class="nav-link" asp-action="Create" asp-controller="Categories">Create Categories</a></li>
                            </ul>
                        </div>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" data-bs-toggle="collapse" href="#ui-subcategories" aria-expanded="false" aria-controls="ui-subcategories">
                            <i class="mdi mdi-table-large menu-icon"></i>
                            @*  *@
                            <span class="menu-title">Subcategories</span>
                            <i class="menu-arrow"></i>
                        </a>
                        <div class="collapse" id="ui-subcategories">
                            <ul class="nav flex-column sub-menu">
                                <li class="nav-item"> <a class="nav-link" asp-action="GetAllSubCategories" asp-controller="SubCategories">All Subcategories</a></li>
                                <li class="nav-item"> <a class="nav-link" asp-action="Create" asp-controller="SubCategories">Create Subcategories</a></li>
                            </ul>
                        </div>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" data-bs-toggle="collapse" href="#ui-suppliers" aria-expanded="false" aria-controls="ui-suppliers">
                            <i class="mdi mdi-pound-box menu-icon"></i>
                            @*  *@
                            <span class="menu-title">Suppliers</span>
                            <i class="menu-arrow"></i>
                        </a>
                        <div class="collapse" id="ui-suppliers">
                            <ul class="nav flex-column sub-menu">
                                <li class="nav-item"> <a class="nav-link" asp-action="GetAllSuppliers" asp-controller="Suppliers">All Suppliers</a></li>
                                <li class="nav-item"> <a class="nav-link" asp-action="Create" asp-controller="Suppliers">Create Suppliers</a></li>
                            </ul>
                        </div>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" data-bs-toggle="collapse" href="#ui-shippers" aria-expanded="false" aria-controls="ui-shippers">
                            <i class="mdi mdi-truck menu-icon"></i>
                            @*  *@
                            <span class="menu-title">Shippers</span>
                            <i class="menu-arrow"></i>
                        </a>
                        <div class="collapse" id="ui-shippers">
                            <ul class="nav flex-column sub-menu">
                                <li class="nav-item"> <a class="nav-link" asp-action="GetAllShippers" asp-controller="Shippers">All Shippers</a></li>
                                <li class="nav-item"> <a class="nav-link" asp-action="Create" asp-controller="Shippers">Create Shippers</a></li>
                            </ul>
                        </div>
                    </li>
                    @* <li class="nav-item">
                    <a class="nav-link" href="documentation/documentation.html">
                    <i class="mdi mdi-file-document-box-outline menu-icon"></i>
                    <span class="menu-title">Documentation</span>
                    </a>
                    </li> *@
                </ul>
            </nav>
            <!-- partial -->
            <div class="main-panel">
                <div class="content-wrapper">
                    @RenderBody()
                </div>
                <!-- content-wrapper ends -->
                <!-- partial:partials/_footer.html -->
                <footer class="footer">
                    <div class="d-sm-flex justify-content-center justify-content-sm-between">
                        <span class="text-muted text-center text-sm-left d-block d-sm-inline-block">Copyright © <a href="https://www.bootstrapdash.com/" target="_blank">bootstrapdash.com </a>2021</span>
                        <span class="float-none float-sm-right d-block mt-1 mt-sm-0 text-center">Only the best <a href="https://www.bootstrapdash.com/" target="_blank"> Bootstrap dashboard  </a> templates</span>
                    </div>
                </footer>
                <!-- partial -->
            </div>
            <!-- main-panel ends -->
        </div>
        <!-- page-body-wrapper ends -->
    </div>
    <!-- container-scroller -->
    <!-- plugins:js -->
    <script src="~/admin/vendors/base/vendor.bundle.base.js"></script>
    <!-- endinject -->
    <!-- Plugin js for this page-->
    <script src="~/admin/vendors/chart.js/Chart.min.js"></script>
    <script src="~/admin/vendors/datatables.net/jquery.dataTables.js"></script>
    <script src="~/admin/vendors/datatables.net-bs4/dataTables.bootstrap4.js"></script>
    <!-- End plugin js for this page-->
    <!-- inject:js -->
    <script src="~/admin/js/off-canvas.js"></script>
    <script src="~/admin/js/hoverable-collapse.js"></script>
    <script src="~/admin/js/template.js"></script>
    <!-- endinject -->
    <!-- Custom js for this page-->
    <script src="~/admin/js/dashboard.js"></script>
    <script src="~/admin/js/data-table.js"></script>
    <script src="~/admin/js/jquery.dataTables.js"></script>
    <script src="~/admin/js/dataTables.bootstrap4.js"></script>

    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.min.js"></script>
@*     @RenderSection("UserEditJs", false)
    @RenderSection("GetAllUsersJs", false)
    @RenderSection("ChangeRoleJs", false) *@
    <!-- End custom js for this page-->
    @RenderSection("GetAllUsersJs", false)
    @RenderSection("UpdateBlogtJs", false)
    @RenderSection("GetAllProductsJs", false)
    @RenderSection("GetAllCategoriesJs", false)
    @RenderSection("GetAllSubCategoriesJs", false)
    @RenderSection("GetAllSubscribersJs", false)
    @RenderSection("GetAllSuppliersJs", false)
    @RenderSection("GetAllOrdersJs", false)
    @RenderSection("GetAllOrders2Js", false)
    @RenderSection("GetAllOrders3Js", false)
    @RenderSection("GetAllBlogsJs", false)
    @RenderSection("GetAllMessagesJs", false)
    @RenderSection("ProfileEditJs", false)
    @RenderSection("UserEditJs", false)
@*     @RenderSection("CreateJs", false)
    @RenderSection("Create2Js", false) *@
    <script src="js/jquery.cookie.js" type="text/javascript"></script>
</body>

</html>

